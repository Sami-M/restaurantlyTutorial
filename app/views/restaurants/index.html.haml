.row
.large-8.columns.large-centered
  %h3.subheader.center
    Restaurantly Spots!
.row
.large-8.columns.large-centered
  - @restaurants.each do |restaurant|
    %h5.subheader
      = restaurant.name
      = link_to "edit", edit_restaurant_path(restaurant)
      = link_to "destroy", restaurant_path(restaurant), method: :delete